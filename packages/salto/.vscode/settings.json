// Place your settings in this file to overwrite default and user settings.
{
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/dist": true,
        "**/coverge": true
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        {
            "language": "typescript",
            "autoFix": true
        },
        {
            "language": "typescriptreact",
            "autoFix": true
        },
    ],
    "tslint.exclude": "**/node_modules/**/*",
    "appService.zipIgnorePattern": [
        ".vscode{,/**}"
    ],
    "appService.deploySubpath": ""
}