{
  "name": "salto",
  "version": "0.1.0",
  "description": "Salto core and CLI",
  "repository": {
    "type": "git",
    "url": "https://github.com/salto-io/salto"
  },
  "scripts": {
    "build": "npm run build-ts && npm run lint",
